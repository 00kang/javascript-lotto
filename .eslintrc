{
    "plugins": ["prettier"],
    "extends": [
        "airbnb-base", 
        "eslint:recommended",
        "plugin:prettier/recommended",
        "plugin:cypress/recommended"
    ],
    "rules": {
        "prettier/prettier": ["error", {
            "endOfLine": "auto"
        }],
        "max-depth": ["error", 2]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 12
    },
    "env": {
        "browser": true,
        "node": true,
    }
}